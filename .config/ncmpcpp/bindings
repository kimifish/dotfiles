def_key "j"
  scroll_down

def_key "k"
  scroll_up

def_key "K"
  select_item
  scroll_up

def_key "J"
  select_item
  scroll_down

def_key "l"
  next_column

def_key "h"
  previous_column

def_key "h"
  jump_to_parent_directory

def_key "l"
  enter_directory

def_key "G"
  move_end

def_key "g"
  move_home

def_key "p"
  pause

def_key "d"
  delete_playlist_items
